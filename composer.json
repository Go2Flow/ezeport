{
    "name": "go2flow/ezport",
    "description": "An importer/exporter package",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "importer",
        "exporter"
    ],

    "authors": [
        {
            "name" : "Jelte ten Holt",
            "email" : "jelte.tenholt@go2flow.de",
            "homepage": "https://www.go2flow.de/",
            "role" : "developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Go2Flow\\Ezport\\EzportServiceProvider",
                "Go2Flow\\Ezport\\EzportContentTypeServiceProvider",
                "Go2Flow\\Ezport\\EzportScheduleServiceProvider"
            ]
        }
    },

    "autoload" : {
        "psr-4" : {
            "Go2Flow\\Ezport\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Go2Flow\\Ezport\\Tests\\": "tests/"
        }
    },

    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0 || ^12.0",
        "guzzlehttp/guzzle": "^7.2",
        "maatwebsite/excel": "^3.1",
        "laravel/horizon": "^5.21",
        "laravel/prompts": "^0.3.0",
        "orchestra/parser": "^9.0 || ^10.0",
        "saloonphp/xml-wrangler": "^1.3",
        "league/flysystem-ftp": "^3.0",
        "league/flysystem-sftp-v3": "^3.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^2.0",
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^9.0 || ^10.0",
        "mockery/mockery": "^1.6"
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse",
        "format": "pint"
    }
}